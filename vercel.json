{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "http://pdfspacer.com/:path*", "destination": "https://pdfspacer.com/:path*", "permanent": true },
    { "source": "https://www.pdfspacer.com/:path*", "destination": "https://pdfspacer.com/:path*", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|gif|svg)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
